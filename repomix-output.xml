                        onError={(e) => {
                            // Fallback logic
                            if (e.target.src !== originalImageUrl && originalImageUrl !== placeholderImage) {
                                e.target.src = originalImageUrl;
                            } else if (e.target.src !== placeholderImage) {
                                e.target.onerror = null;
                                e.target.src = placeholderImage;
